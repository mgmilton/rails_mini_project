<h2> All Ideas </h2>

<% @ideas.each do |idea| %>
  <%= link_to idea.title, user_idea_path(idea.user, idea) %>
  <%= link_to "Edit", edit_user_idea_path(idea.user, idea) %>
  <%= link_to "Delete", user_idea_path(idea.user, idea), method:
  "delete" %> </br>
  <%= idea.description %> </br> </br>
<% end %>

<%= link_to "Create an Idea", new_user_idea_path %>
