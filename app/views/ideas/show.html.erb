<html>
<h3> <%= @idea.title %></h3>
<p>Description: <%= @idea.description %> </p>
<p>Category: <%= @idea.category.name %> </p>
<p><%= link_to "Edit", edit_user_idea_path(@idea.user, @idea) %>
<%= link_to "Delete", user_idea_path(@idea.user, @idea), method: :delete%> </p>
<p> <% @idea.images.each do |image| %>
     <img src="<%= image.url %>">
     <% end %> </p>
</html>
