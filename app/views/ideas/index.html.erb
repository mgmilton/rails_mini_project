<h2> All Ideas </h2>

<% @ideas.each do |idea| %>
  <%= link_to idea.title, idea_path(idea) %>
  <%= link_to "Edit", edit_idea_path(idea) %>
  <%= link_to "Delete", idea_path(idea), method:
  "delete" %> </br>
  <%= idea.description %> </br> </br>
<% end %>

<%= link_to "Create an Idea", new_idea_path %>
